


<div class="row">
  <div class="col-sm-6">
    <h1>Contact_messages</h1>
  </div>

  <div class="col-sm-6 text-right">
  <%= link_to new_contact_message_path, class: 'btn btn-primary' do %>
    Add New Contact message
  <% end %>
  </div>
</div>

<div class="table-responsive">
  <table class="table table-striped table-bordered table-hover">
    <thead>
      <tr>
    

            <th>First name</th>
            <th>Last name</th>
            <th>Email</th>
            <th>Phone number</th>
            <th>Subject</th>
            <th>Message</th>
            
          <th></th>
        
      </tr>
    </thead>

    <tbody>
      <% @contact_messages.each do |contact_message| %>
        <%= content_tag :tr, id: dom_id(contact_message), class: dom_class(contact_message) do %>
          

                      <td><%= contact_message.first_name %></td>
                      <td><%= contact_message.last_name %></td>
                      <td><%= contact_message.email %></td>
                      <td><%= contact_message.phone_number %></td>
                      <td><%= contact_message.subject %></td>
                      <td><%= contact_message.message %></td>
          
          
            <td><%= link_to 'Show', contact_message %></td>
          
        <% end %>
      <% end %>
    </tbody>
  </table>
</div>
