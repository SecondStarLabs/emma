<div class="page-header">
  <%= link_to blog_lists_path, class: 'btn btn-default' do %>
    All Blog_lists
  <% end %>
  <h1><%= @list.name %> @list.name</h1>
</div>

<dl class="dl-horizontal">
  <dt>Name:</dt>
  <dd><%= @list.name %></dd>

  <dt>Position:</dt>
  <dd><%= @list.position %></dd>

  <dt>Cards</dt>
  <ul>
    <% @list.cards.each do |card| %>
      <li>
        <%= link_to card.name, edit_blog_card_path(card)  %>
      </li>
      <li><%= card.name %>  </li>
    <% end %>
  </ul>
</dl>
