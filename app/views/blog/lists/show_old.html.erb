<div class="page-header">
  <%= link_to blog_lists_path, class: 'btn btn-default' do %>
    All Blog_lists
  <% end %>
  <%= link_to edit_blog_list_path(@blog_list), class: 'btn btn-primary' do %>
    Edit
  <% end %>
  <h1>Show blog_list</h1>
</div>

<dl class="dl-horizontal">
  <dt>Name:</dt>
  <dd><%= @blog_list.name %></dd>

  <dt>Position:</dt>
  <dd><%= @blog_list.position %></dd>

</dl>
